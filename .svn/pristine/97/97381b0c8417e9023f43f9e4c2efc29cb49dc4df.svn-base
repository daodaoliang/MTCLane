#打印标识，0-不打印车牌 1-打印车牌（默认0）
printflag=0
#ETC卡发行方验证标识 0-仅验证前8位 1-验证全部16位
issuerflag = 0
#电脑键盘启用标识  0-不启用 1-启用（默认0）
pcKeyboardCode=1
#新国标ETC卡车型差异处理标识 0-入口etc卡车型差异不处理，出口etc卡车型差异走监控室确认流程 1-出入口提示车型差异，要求司机出示正确ETC卡或收费员修改车型
#本标识只针对ETC卡版本号以4开头的的新国标ETC卡，对于旧国标ETC卡（版本号为10）按原有流程处理（默认0）
etcClassDiffFlag=0
#1-报警器报警代码31; 2-报警器报警代码32
alarm=1
########################################################################################################################
#出口ETC重放行使用                                                                                                      #
########################################################################################################################
#ETC卡出口检验放行标识 0-不启用 1-启用（默认0）
etcExitCode=1
#ETC出口检验放行时是否记录简单交易流水 0-不记录 1-记录（默认0）
etcExitListCode=1
########################################################################################################################
#代收流程标志（机场高速）0-不启用 1-启用（默认0）
collectFlag=0
#车型差异车牌差异跳最远站标识,0-不跳最远站，其他-跳最远站（默认0）
diffFlag=0
#下班状态下发现升级文件是否自动升级，0-自动升级，1-不自动升级（默认0）
autoUpdateFlag=0
#拖车标识符0-自动拆分拖车，1-手动拆分拖车(默认0)
towFlag=0
########################################################################################################################
#自助取卡用                                                                                                             #
########################################################################################################################
#自助发卡机标识 0-不启用自助发卡机 1-启用自助发卡机（默认0）
autoMachineFlag=1
#自助取卡防盗车取卡功能启用标识 0-不启用 1-启用(默认0)
AntiVehBackFlag=1
#自助发卡车道黑名单车辆语音提示，0-不支持 1-支持，默认1
autoBlackVehAlert=1
#自助发卡车道灰名单车辆语音提示，0-不支持 1-支持，默认1
autoGrayVehAlert=1
#自助发卡车道凌晨禁止大客车取卡功能0-不启用 1-启用（默认0）
autoForbidFunction=1
#自助发卡车道凌晨禁止大客车取卡功能作用时间段,格式HHmm-HHmm(24小时制,最大为2400,注：类似于0199之类的设置为错误设置),有多个时间段时，以逗号分隔
autoForbidTime=0200-0500
#自助发卡车道凌晨禁止大客车取卡功能作用车型,多个车型时以逗号分隔
autoForbidVehClass=2,3,4,5
########################################################################################################################
#通行卡加密用                                                                                                           #
########################################################################################################################
#通行卡加密功能 0-启用 1-不启用 默认1
encryptFunction=0
#通行卡加密校验失败处理功能， 0-给出提示，由收费员决定是否可以使用 1-允许使用，默认1
encryptErrorFunction=0
#是否启用通行卡未加密处理功能， 0-给出提示，由收费员决定是否可以使用 1-允许使用，默认1
unEncryptFunction=0
#######################################################################################################################
#入口支持6型车输入，0-支持 1-不支持，默认0
entry6Flag=0
#纸券功能，0-支持 1-不支持，默认0
paperCardFlag=0
#在线收费功能 0-启用 1-不启用 默认1
onlineMode=1
#模拟通行监控室确认 0-启用 1-不启用 默认1
simuConfrimFlag=0
#字轨号输入功能 0-启用 1-不启用 默认1
trackFunction=1
#出口是否启用全车牌校验规则，1-启用 0-不启用 （默认0）
fullVehPlateCheckFlag=0
######################################################################
#数据编码模式，0-湖南，1-广东（默认湖南）
dataEncodeMode=0
#人工发卡自助发卡机特殊模式 0-郎为 1-优创 2-西埃斯 默认0
manualAutoLevel=0
#人工发卡车道接入自助发卡机 0-启用 1-不启用，默认1
manualAutoFunction=1
#记录临时流水方案 0-优化前 1-优化后，默认1
tempFunction=1
#ETC读写器读写M1卡功能  0-启用 1-不启用 默认1
etcTscFunction=0

