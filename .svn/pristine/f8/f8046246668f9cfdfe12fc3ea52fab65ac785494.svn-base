#﻿#打印标识，0不打印车牌 1 打印车牌（默认0）
printflag=0
#ETC卡发行方验证标识 0仅验证前8位 1验证全部16位
issuerflag = 0
#电脑键盘启用标识  0不启用 1 启用（默认0）
pcKeyboardCode=0
#新国标ETC卡车型差异处理标识 （默认0）
#0 入口etc卡车型差异不处理，出口etc卡车型差异走监控室确认流程 
#1 出入口提示车型差异，要求司机出示正确ETC卡或收费员修改车型
#本标识只针对ETC卡版本号以4开头的的新国标ETC卡，对于旧国标ETC卡（版本号为10）按原有流程处理
etcClassDiffFlag=0
#1报警器报警代码31; 2 报警器报警代码32
alarm=1
########################################################################################################################
#出口ETC重放行使用                                                                                                      #
########################################################################################################################
#ETC卡出口检验放行标识 0不启用 1 启用（默认0）
etcExitCode=0
#ETC出口检验放行时是否记录简单交易流水 0 不记录 1 记录（默认0）
etcExitListCode=0
########################################################################################################################
#代收流程标志（机场高速）0不启用 1启用（默认0）
collectFlag=0
#车型差异车牌差异跳最远站标识,0-不跳最远站，其他-跳最远站（默认0）
diffFlag=0
#下班状态下发现升级文件是否自动升级，0-自动升级，1-不自动升级（默认0）
autoUpdateFlag=0
#拖车标识符0自动拆分拖车，1手动拆分拖车(默认0)
towFlag=0
########################################################################################################################
#自助取卡用                                                                                                             #
########################################################################################################################
#自助发卡功能启用标识 0 不启用自助发卡机 1 启用自助发卡机（默认0）
autoMachineFlag=0
#自助取卡防倒车取卡功能启用标识 0不启用 1启用(默认0)
AntiVehBackFlag=0
#自助发卡车道黑名单车辆语音提示，0不支持 1支持，默认1
autoBlackVehAlert=1
#自助发卡车道灰名单车辆语音提示，0不支持 1支持，默认1
autoGrayVehAlert=1
#自助发卡车道凌晨禁止大客车取卡功能0-不启用 1-启用（默认0）
autoForbidFunction=0
#自助发卡车道凌晨禁止大客车取卡功能作用时间段,格式HHmm-HHmm(24小时制,最大为2400,注：类似于0199之类的设置为错误设置),有多个时间段时，以逗号分隔
autoForbidTime=0200-0500
#自助发卡车道凌晨禁止大客车取卡功能作用车型,多个车型时以逗号分隔
autoForbidVehClass=2,3,4,5
########################################################################################################################
#通行卡加密用                                                                                                           #
########################################################################################################################
#通行卡加密功能 0 启用 1 不启用 默认1
encryptFunction=1
#是否启用通行卡加密校验失败处理功能， 0启用 1 不启用，默认1
encryptErrorFunction=1
#是否启用通行卡未加密处理功能， 0启用 1 不启用，默认1
unEncryptFunction=1
#######################################################################################################################
#入口支持6型车输入，0支持 1 不支持，默认0
entry6Flag=0
#纸券功能，0支持 1不支持，默认0
paperCardFlag=0
#在线收费功能 0启用 1不启用 默认1
onlineMode=1
#模拟通行监控室确认 0 启用 1不启用 默认1
simuConfrimFlag=1
#字轨号输入功能 0 启用 1 不启用 默认1
trackFunction=1
#出口是否启用全车牌校验规则，1-启用 0-不启用 （默认0）
fullVehPlateCheckFlag=0
######################################################################
#数据编码模式，0-湖南，1-广东（默认湖南）
dataEncodeMode=0
#人工发卡自助发卡机特殊模式 0 郎为 1 优创 2西埃斯 默认0
manualAutoLevel=0
#人工发卡车道接入自助发卡机 0-启用 1-不启用，默认1
manualAutoFunction=1
#记录临时流水方案 0-优化前 1-优化后，默认1
tempFunction=1
#ETC读写器读写M1卡功能  0-启用 1-不启用 默认1
etcTscFunction=1
#车牌识别模式 0-加锁 1-不加锁（默认0）
lprmode=0
#费显异常报警功能 0启用 1不启用(默认1)
TFIErrorAlarm=1
########################################################################################################################
#货车湘通储值卡打折                                                                                                      #
########################################################################################################################
#是否启用货车湘通储值卡打折功能 0-启用 1-不启用，默认0
truckETCDisFun=0
#合法装载货车湘通储值卡打折折扣率,默认0.9
truckETCDisRate1=0.9
#合法装载货车非湘通储值卡打折折扣率，默认0.98
truckETCDisRate2=0.98
#货车ETC储值卡打折开始时间,格式为yyyy-MM-dd HH:mm:ss，默认2016-10-01 00:00:00
truckETCDisStartTime=2016-10-01 00:00:00
#货车ETC储值卡打折结束时间,格式为yyyy-MM-dd HH:mm:ss，默认2017-10-01 00:00:00
truckETCDisEndTime=2017-10-01 00:00:00
########################################################################################################################
#车牌识别抓拍车牌模式，默认0
#0 循环抓拍，无结果自动重新抓拍
#1 车辆到达线圈后或车型确认后开始抓拍，无结果需要手动重新输入车型或按【00】键手动抓拍
lprPlateMode=0
#入口已发通行卡再刷ETC卡的功能 0-启用 1-不启用 默认0
cscToEtcFun=0
#出口现金付款时必须输入收取通行费并且输入金额需大于等于应收通行费功能 0-启用 1-不启用 默认1
fareInputFun=1
#用于配置抓拍车牌后是否获取第一次车牌结果 0-不获取 1-获取，默认0（注：汉王车牌识别设备需要将此项设为1）
lprCaptureMode=0
#是否启用支持河南紫光捷通、清华紫光计重设备删除指令 0-启用 1-不启用 默认1（注：只在存在紫光捷通、清华紫光计重设备车道启用。）
unisPAT=1
#是否启用兼容紫凯费显设备。0-启用 1-不启用 默认1（注：只在存在紫凯费显设备的车道根据现场实际情况进行启用，该功能只针对GEA功能费显。）
ZKDTFI=1
#是否启用全键盘 0-启用 1-不启用 默认1
fullKeyboardFunc=1
#和上一次工班的时间间隔检测，默认是28800秒，即8小时
checkTimeInterval=28800
#VC4000视频采集卡视频接口位置，默认1（1，2，3，4分别对应VC4000由外至内四个视频接口）
videoSocketNum=1
