#常量配置表
#硬件异常检测时间间隔(秒)，默认3秒
tollErrorInterval=3
#记录程序内存使用情况时间间隔（秒），默认5秒
memoryInterval=5
#出口检测ETC卡是否在本站进行交易的时间间隔(分钟),默认10分钟
etcExitInterval=10
#费率升级文件出现异常时的提示时间间隔（分钟），默认5分钟
paramAlertInterval=5
#等待M1读卡器响应的时间，单位：毫秒，默认250毫秒
mifReadInterval=250
#日志存储失效日期,单位：天，默认：365天
expiresLogDays=365
#车牌识别超时时间
lprInterval=1
#代收通行费路段（默认为空）
collectRoad=机场高速
######################################################################################################
#######################################在线式收费配置##################################################
######################################################################################################
#离线收费倒计时整体时间，单位：hh:mm:ss，默认48小时
totalOffLineTime=48:00:00
#向省中心TCO发送车道电脑内存信息的时间间隔，单位：秒，默认60秒
lanePcInfoInterval=60
######################################################################################################
#######################################监控室通信配置##################################################
######################################################################################################
#出口获取入口图片超时时间(毫秒)，默认3000毫秒
getEntIMGInterval=3000
#请求入口图片服务器最大数量
maxImgSocketCnt=10
#最大监控室连接数量
maxMonitorCnt=10
#监控室确认超时时间(秒),默认30秒
overTimeInterval=30
#与监控室通信心跳检测时间间隔（毫秒），默认2000毫秒
heartBeatInterval=2000
#与监控室通信日志记录 0 普通 1 详细
monitorLogLevel=1
######################################################################################################
#######################################自助发卡配置##################################################
######################################################################################################
#自助发卡模式下人工修改车牌自动确认时间，默认15秒，最少1秒（单位：秒）
autoPlateInterva11=15
#自助发卡模式下等待车牌识别返回车牌识别结果的最大时间，默认2秒,最少1秒（单位：秒）
autoPlateInterva12=2
#自助发卡模式下车型确认后前线圈未被激活，超过指定时间后自动取消当前车辆信息，默认5分钟，最少1分钟(单位：分钟)
autoClassInterval=5
#自助发卡请取卡语音提示时间间隔，默认5秒，最少1秒（单位：秒）
autoAudioInterval1=5
#自助发卡谢谢一路顺风语音提示时间间隔，默认10秒，最少1秒（单位：秒）
autoAudioInterval2=10
#自助取卡倒车语音提示时间间隔，默认5秒，最少1秒，单位：秒
autoBackAlertInterval=5
#车辆离开后多长时间内检测到的倒车行为需要进行锁定，默认5秒，最少1秒，单位：秒
autoBackDetectInterval=5
######################################################################################################
#######################################西埃斯自助卡机模拟人工卡机配置###################################
######################################################################################################
#西埃斯自助卡机模拟人工卡机时需要收到卡机回应的指令，以逗号分隔
XasCicmOrders=0A5330303031310D,0A5330303131310D,0A5330313031310D,0A5330313131310D,0A5331303031310D,0A5331303131310D,0A5331313031310D,0A5331313131310D
