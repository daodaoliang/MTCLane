package com.hgits.vo;

/**
 * 车货轴型总限重表
 * 
 * @author wh
 *
 */
public class TruckToallLimit {
	private Integer carAxisNum;
	private Integer carLimitWeight;
	/**
	 * @return the carAxisNum
	 */
	public Integer getCarAxisNum() {
		return carAxisNum;
	}
	/**
	 * @param carAxisNum the carAxisNum to set
	 */
	public void setCarAxisNum(Integer carAxisNum) {
		this.carAxisNum = carAxisNum;
	}
	/**
	 * @return the carLimitWeight
	 */
	public Integer getCarLimitWeight() {
		return carLimitWeight;
	}
	/**
	 * @param carLimitWeight the carLimitWeight to set
	 */
	public void setCarLimitWeight(Integer carLimitWeight) {
		this.carLimitWeight = carLimitWeight;
	}
}
